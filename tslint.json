{
  "extends": ["tslint:latest", "tslint-react"],
  "linterOptions": {"exclude": ["config/**/*.js", "node_modules/**/*.ts", "coverage/lcov-report/*.js"]},
  "rules": {
    "quotemark": [false],
    "member-access": false,
    "jsx-no-multiline-js": false,
    "trailing-comma": false,
    "arrow-parens": false,
    "ordered-imports": false
  }
}